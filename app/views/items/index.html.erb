<% @items.each do |item|%>
  <div class="item">
    <%= link_to image_tag(item.image,:class => "figure-img img-fluid z-depth-1",:style => "max-width: 700px;"),item_path(item)%>
    <strong>Price:</strong><span><%= item.title%></span><br>
    <strong>Price:</strong><span><%= item.description%></span><br>
    <strong>Price:</strong><span><%= item.price%></span><br>
    <%= render 'item_links',item: item%>
  </div>

<%end%>
